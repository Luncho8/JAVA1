package sec02.exam05;

public class StringExample {
    public static void main(String[] args) {
    	String name = "홍길동";
    	String job = "프로그래머";
    	String str = job.substring(0,1);
    	char c = job.charAt(1);
    	System.out.println(name);
    	System.out.println(job);
    	System.out.println(str);
    	System.out.println(c);
    }
}
